<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.spider.mapper.MovieMapper">
    <update id="updateByMovieId">
        update movie set()
    </update>

    <select id="findByMovieName" resultType="com.spider.entity.Movie">
        select * from movie where movieName like "%"#{name}"%"
    </select>

</mapper>